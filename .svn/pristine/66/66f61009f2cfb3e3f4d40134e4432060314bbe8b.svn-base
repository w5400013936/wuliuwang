    <view class='details-li-info'>2018-08-13</view>
  </view>
  <view class='details-li'>
    <view class='details-li-text'>运价</view>
    <view class='details-li-info'>面议</view>
  </view>
  <view class='details-li'>
    <view class='details-li-text'>常用语</view>
    <view class='details-li-info'>求货源</view>
  </view>
  <view class='details-li'>
    <view class='details-li-text'>即时/长期</view>
    <view class='details-li-info'>即时车源</view>
  </view>
  <view class='details-li'>
    <view class='details-li-text'>单程/往返</view>
    <view class='details-li-info'>往返</view>
  </view>
  <view class='details-li'>
    <view class='details-li-text'>说明</view>
    <view class='details-li-info'>求货源</view>
  </view>
