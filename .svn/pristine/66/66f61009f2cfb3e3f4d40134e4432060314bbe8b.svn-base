    <view class='details-li-text'>车主联系电话</view>
    <view class='details-li-info'>18826138850</view>
  </view>
  <view class='details-li'>
    <view class='details-li-text'>车辆识别代号</view>
    <view class='details-li-info'></view>
  </view>
