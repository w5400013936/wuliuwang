/* pages/changePwd/changePwd.wxss */
.main{
  height: 100%;
  padding: 150rpx 26rpx 0 26rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.cell{
  height: 90rpx;
  border-bottom: 1rpx solid #ddd;
  margin-bottom: 20rpx;
}
.cell input{
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  font-size: 28rpx;
}
.submit-btn{ margin-top: 170rpx; width: 667rpx; line-height: 90rpx; text-align: center; background: #ddd; color: #fff; font-size: 30rpx;border-radius: 0;}
.submit-btn.on{
  background: #68c5cd;
}