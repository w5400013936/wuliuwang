/**app.wxss**/
page{ background: #eeeeee;height: 100%;}
.clear{ clear: both;}
.height27{ height: 27rpx;}
.bgwhite{ background: #fff;}
.mainColor{color: #68c5cd;}
.fl{float: left;}
.fr{float: right;}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.bottom-box{
  width: 100%;
  height: 90rpx;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  text-align: center;
  line-height: 90rpx;
  font-size: 21rpx;
  color: #ffffff;
  background-color: #69c5cd;
}
.list-li-input input{
  line-height: normal !important;
}
