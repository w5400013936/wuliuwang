/* pages/network-manage/network-manage.wxss */
view{
  box-sizing: border-box;
}
checkbox .wx-checkbox-input{
   width: 25rpx;
   height: 25rpx; 
}
.header{
  height: 68rpx;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 100;
  background-color: #68c5cd;
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
  color: #fff;
  padding: 0 25rpx;
  font-size: 26rpx;
}
.main{
  height: calc(100% - 110rpx);
  padding-top: 68rpx;
  box-sizing: border-box;
}
.checkbox image{
  width: 27rpx;
  height: 27rpx;
  vertical-align: middle;
}
.pageNav{
  width: 250rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 70rpx;
}
.operation{
  flex: 1;
}
.operation-del{
  float: right;
}
.operation-refresh{
  float: right;
  margin-left: 50rpx;
}
.list{
  margin-top: 10rpx;
  position: relative;
}
.list-container{
  height: 245rpx;
  background-color: #fff;
  padding: 16rpx 90rpx 0 90rpx;
  position: relative;
  z-index: 10;
}
.list-title{
  width: 100%;
  font-size: 29rpx;
  /* color: #555555; */
  color: #000;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.list-content{
  margin-top: 12rpx;
  font-size: 25rpx;
  color: #666666;
}
.list-address{
  margin-bottom: 8rpx;
}
.list-manager{
  font-size: 25rpx;
  color: #666666;
  margin-top: 14rpx;
}
.list-time{
  font-size: 25rpx;
  color: #666666;
  margin-top: 12rpx;
}
.list-checkbox{
  position: absolute;
  top: 10rpx;
  left: 27rpx;
}
.list-ico{
  width: 34rpx;
  height: 34rpx;
  position: absolute;
  top: 17rpx;
  right: 32rpx;
}
.list-btns{
  text-align: center;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 5;
}
.list-edit{
  display: inline-block;
  width: 208rpx;
  height: 100%;
  background-color: #dddddd;
  font-size: 33rpx;
  color: #fefefe;
  line-height: 245rpx;
}
.list-del{
  display: inline-block;
  width: 208rpx;
  height: 100%;
  background-color: #e60012;
  font-size: 33rpx;
  color: #fefefe;
  line-height: 245rpx;
}
