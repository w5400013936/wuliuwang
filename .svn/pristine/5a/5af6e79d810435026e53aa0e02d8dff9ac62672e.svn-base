        <view class='desc'>
          <view class='manager'>
            <image class='ico' src='/img/name.png'></image>魏经理
          </view>
          <view class='phone'>
            <image class='ico' src='/img/phone.png'></image>18066668888
          </view>
        </view>
      </view>
    </view>
    <view class='header-box'>
      <image class='ico' src='/img/tag.png'></image>
      <view class='tag-box'>
        <view class='tag'>整车运输</view>
        <view class='tag'>整车运输</view>
        <view class='tag'>整车运输</view>
