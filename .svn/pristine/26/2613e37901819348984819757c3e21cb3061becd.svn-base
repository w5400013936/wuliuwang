<view class='header'>
  <view class='checkbox' bindtap='changeSelect'>
    <image src='{{select?"/img/selected.png":"/img/unselected.png"}}'></image>
    全选
  </view>
  <view class='pageNav'>
    <view>上一页</view>
    <view>1</view>
    <view>下一页</view>
  </view>
