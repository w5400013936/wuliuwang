/* pages/company/company.wxss */
 .search-box{ padding-top: 16rpx;} 
.search-form{ width:80%; margin: 0 auto; height: 62rpx;background-color: #fff;  border-radius: 8rpx; overflow: hidden; position: relative; padding-left:70rpx;}
.search-form icon{ width: 26rpx; height: 26rpx; position: absolute; left: 28rpx; top: 15rpx; color: #fff;}
.search-form input{ font-size: 26rpx; line-height: 70rpx; margin-top: 8rpx;}


.vam{
  vertical-align: middle;
}
.dib{
  display: inline-block;
}
.mr70{
  margin-right: 70rpx;
}
.ellipsis{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.w450{
  width: 450rpx;
}
.cell{
  margin: 22rpx auto;
  width: 90%;
  background-color: #fff;
  border-radius: 8rpx;
}
.cell-container{
  width: 100%;
  height: 180rpx;
  display: flex;
  align-items: center;
  padding-left: 15rpx;
  box-sizing: border-box;
}
.cell-left{
  margin-right: 17rpx;
}
.cell-left-img{
  width: 104rpx;
  height: 104rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.cell-right{
  flex: 1;
}
.cell-right-header{
  font-size: 33rpx;
}
.cell-right-img{
  width: 44rpx;
  height: 23rpx;
  margin-left: 18rpx;
  vertical-align: middle;
}
.cell-right-footer{
  margin-top: 26rpx;
  font-size: 25rpx;
}
.cell-ico{
  width: 49rpx;
  height: 49rpx;
  vertical-align: middle;
  margin-right: 13rpx;
}
.cell-footer{
  background-color: #fafafa;
  border-top: 1rpx solid #ddd;
  padding: 14rpx 20rpx;
  font-size: 25rpx;
  display: flex;
  color: #999;
}
.cell-footer-text{
  flex: 1;
  padding-top: 6rpx;
}

