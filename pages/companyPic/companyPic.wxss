/* pages/companyPic/companyPic.wxss */
.cell{
  height: 158rpx;
  margin-top: 8rpx;
  position: relative;
}
.cell-container{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  z-index: 10;
  position: relative;
  background-color: #fff;
  padding-left: 24rpx;
}
.cell-left{
  font-size: 33rpx;
  color: #666666;
}
.cell-left image{
  width: 110rpx;
  height: 110rpx;
  margin-right: 38rpx;
  vertical-align: middle;
}
.cell-btns{
  text-align: center;
  position: absolute;
  top: 0;
  right: 0;
  z-index: 5;
}
.cell-del{
  display: inline-block;
  width: 208rpx;
  height: 100%;
  background-color: #e60012;
  font-size: 33rpx;
  color: #fefefe;
  line-height: 158rpx;
}
.main{
  height: calc(100% - 90rpx);
  box-sizing: border-box;
}