<!--pages/setting/setting.wxml-->
<view class='list bgwhite'>
  <view class='list-li' bindtap='goChangePwd'>
    <view class='list-li-text'>修改密码</view>
    <view class='ico arrow-down'>
      <image src='/img/arrow-right.png' mode="aspectFill"></image>
    </view>
    <view class='clear'></view>
  </view>
  <view class='list-li' bindtap='goFeedback'>
    <view class='list-li-text'>意见反馈</view>
    <view class='ico arrow-down'>
      <image src='/img/arrow-right.png' mode="aspectFill"></image>
    </view>
    <view class='clear'></view>
  </view>
</view>
<view class='list bgwhite'>
  <view class='list-con' bindtap='exitTap'>安全退出</view>
</view>